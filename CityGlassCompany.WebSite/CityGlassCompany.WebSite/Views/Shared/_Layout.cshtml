<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0 , maximum-scale=1.0, user-scalable=0" />
    <link rel="canonical" href="https://www.cityglasscompany.net/" />
    <link rel='shortlink' href='https://www.cityglasscompany.net/' />
    <title>@ViewData["Title"] - City Glass</title>
    <link href="~/default/img/favicon-32x32.png" rel="icon">
    <link href="~/default/img/favicon-180x180.png" rel="apple-touch-icon">

    <link rel="stylesheet" id="et-builder-googlefonts-cached-css" href="https://fonts.googleapis.com/css?family=Roboto:100,100italic,300,300italic,regular,italic,500,500italic,700,700italic,900,900italic&amp;subset=latin,latin-ext&amp;display=swap" type="text/css" media="all">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <link href="~/default/vendor/icofont/icofont.min.css" rel="stylesheet">
    <link href="~/default/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/default/vendor/venobox/venobox.css" rel="stylesheet">
    <link href="~/default/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/default/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/default/css/style.css" rel="stylesheet" />
    <link href="~/default/css/site.css" rel="stylesheet" />
</head>
<body>

    <partial name="_HeaderSection" />

    @RenderBody()

    <partial name="_FooterSection" />

    <div id="preloader"></div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js" integrity="sha512-bLT0Qm9VnAYZDflyKcBaQ2gg0hSYNQrJ8RilYldYQ1FxQYoCLtUjuuRuZo+fjqhx/qtq/1itJ0C2ejDxltZVFg==" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
    <script src="~/default/vendor/jquery.easing/jquery.easing.min.js"></script>
    <script src="~/default/vendor/waypoints/jquery.waypoints.min.js"></script>
    <script src="~/default/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/default/vendor/venobox/venobox.min.js"></script>
    <script src="~/default/vendor/owl.carousel/owl.carousel.min.js"></script>
    <script src="~/default/vendor/aos/aos.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <!-- Template Main JS File -->
    <script src="~/default/js/main.js"></script>
    <script src="~/default/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <script src="~/default/vendor/layzr/layzr.js"></script>
    <script type="text/javascript">
        var layzr = new Layzr({
            attr: 'data-src',
            retinaAttr: 'data-layzr-retina',
            callback: null
        });
    </script>
</body>
</html>
