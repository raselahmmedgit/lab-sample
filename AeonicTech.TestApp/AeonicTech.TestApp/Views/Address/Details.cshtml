@model AeonicTech.TestApp.ViewModels.AddressEntityViewModel
@{
    ViewData["Title"] = "Address";
    ViewData["SubTitle"] = "Address";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row mb-4">
    <div class="col-10">
        <h1 class="f-26">@ViewData["SubTitle"]</h1>
    </div>
    <div class="col-2 text-right">
        <a href="@Url.Action("Index", "Address", new { Area = "" })" title="Back" class="btn btn-secondary">Back</a>
    </div>
</div>

@Html.RenderAlert("AddressMessage")

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <form action="javascript.void(0);" class="form form-horizontal">

                    <div class="form-body">
                        <div class="form-group row">
                            <label asp-for="AddressLineOne" class="col-3 control-label"></label>
                            <div class="col-4">
                                @Model.AddressLineOne
                            </div>
                        </div>
                        <div class="form-actions form-actions-new">
                            <div class="row">
                                <div class="col-12">
                                    <a href="@Url.Action("Index", "Address", new { Area = "" })" title="Back" class="btn btn-danger">Cancel</a>
                                </div>
                            </div>
                        </div>
                    </div>

                </form>
            </div>
        </div>
    </div>
</div>



