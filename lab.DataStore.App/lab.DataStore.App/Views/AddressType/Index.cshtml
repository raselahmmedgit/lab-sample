@{
    ViewData["Title"] = "Address Type";
}
<div class="row">
    <div class="col-md-12">
        <div class="float-right" style="margin-bottom: 20px;">
            <a href="javascript:;" data-href="@Url.Action("Create", "AddressType", new { Area = "" })" title="Add" onclick="AppModal.AddCommon(this)" class="btn btn-primary">Add New</a>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <table id="dataTablesAddressType" class="table table-striped" style="width:100%"></table>
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script src="~/js/addresstype.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            AddressType.LoadDataTables('dataTablesAddressType', App.DisplayLength(), ("/AddressType/GetDataAsync"));
        });
    </script>
}