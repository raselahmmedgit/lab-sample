@model IEnumerable<lab.DataStore.App.EntityModels.ContactProfile>

@{
    ViewData["Title"] = "Index";
}

<div class="row">
    <div class="col-md-12">
        <div class="float-right" style="margin-bottom: 20px;">
            <a href="javascript:;" data-href="@Url.Action("Create", "ContactProfile", new { Area = "" })" title="Add" class="btn btn-primary">Add New</a>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <table id="dataTablesContactProfile" class="table table-striped" style="width:100%"></table>
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script src="~/js/addresstype.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            ContactProfile.LoadDataTables('dataTablesContactProfile', App.DisplayLength(), ("/ContactProfile/GetDataAsync"));
        });
    </script>
}